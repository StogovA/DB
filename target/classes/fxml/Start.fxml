<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.Controller">
   <children>
      <TableView fx:id="myTableView" layoutX="14.0" layoutY="14.0" prefHeight="200.0" prefWidth="405.0">
         <columns>
            <TableColumn fx:id="colID" prefWidth="50.0" text="Column X" />
            <TableColumn fx:id="colName" prefWidth="100.0" text="Column X" />
            <TableColumn fx:id="colAge" prefWidth="125.0" text="Column X" />
            <TableColumn fx:id="colEmail" prefWidth="125.0" text="Column X" />
         </columns>
      </TableView>
      <Button fx:id="btnEdit" layoutX="430.0" layoutY="188.0" mnemonicParsing="false" onAction="#pressEdit" prefHeight="25.0" prefWidth="59.0" text="Edit" />
      <Button fx:id="btnDelete" layoutX="430.0" layoutY="157.0" mnemonicParsing="false" onAction="#pressDelete" prefHeight="25.0" prefWidth="59.0" text="Delete" />
      <Button fx:id="btnAdd" layoutX="430.0" layoutY="125.0" mnemonicParsing="false" onAction="#pressAdd" prefHeight="25.0" prefWidth="59.0" text="Add" />
   </children>
</AnchorPane>
